{
    "name": "service-msgtemplates-node",
    "version": "1.0.1",
    "author": "Conceptual Vision Consulting LLC",
    "description": "Message templates microservice in Node.js",
    "contributors": [
        {
            "name": "Sergey Seroukhov",
            "message": "seroukhov@gmail.com"
        }
    ],
    "main": "./obj/src/index.js",
    "typings": "./obj/src/index.d.ts",
    "keywords": [
        "pip.services",
        "message template",
        "microservice"
    ],
    "noAnalyze": true,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:pip-services-content2/service-msgtemplates-node.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=14.0.0"
    },
    "private": false,
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "protogen": "cd src/protos && npx grpc_tools_node_protoc  --js_out=import_style=commonjs,binary:./ --grpc_out=.  ./msgtemplates_v1.proto",
        "launch": "node run",
        "relaunch": "tsc && node run",
        "test": "mocha -t 10000 -R spec -u tdd --recursive ./obj/test",
        "retest": "tsc && mocha -t 10000 -R spec -u tdd --recursive ./obj/test",
        "benchmark": "matcha -R clean -I tdd ./obj/benchmark/main.js",
        "rebenchmark": "tsc && matcha -R clean -I tdd ./obj/benchmark/main.js"
    },
    "dependencies": {
        "pip-services3-aws-nodex": "^1.1.*",
        "pip-services3-commons-nodex": "^1.0.*",
        "pip-services3-components-nodex": "^1.4.*",
        "pip-services3-container-nodex": "^1.0.*",
        "pip-services3-data-nodex": "^1.0.*",
        "pip-services3-grpc-nodex": "^1.0.*",
        "pip-services3-mongodb-nodex": "^1.0.*",
        "pip-services3-rpc-nodex": "^1.2.*",
        "pip-services3-swagger-nodex": "^1.0.*"
    },
    "devDependencies": {
        "@types/chai": "*",
        "@types/mocha": "*",
        "@types/mongodb": "^2.1.38",
        "@types/node": "*",
        "chai": "*",
        "google-protobuf": "^3.7.1",
        "grpc_tools_node_protoc_ts": "^5.0.1",
        "mocha": "*",
        "restify": "^4.3.0",
        "ts-protoc-gen": "^0.9.0"
    }
}
